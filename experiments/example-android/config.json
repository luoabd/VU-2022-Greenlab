{
  "type": "web",
  "devices": {
    "s9+": {}
  },
  "repetitions": 1,
  "duration": 60000,
  "monkeyrunner_path": "/opt/android-sdk/tools/bin/monkeyrunner",
  "systrace_path": "/opt/android-sdk/platform-tools/systrace/systrace.py",
  "powerprofile_path": "/home/luoabd/power_profile.xml",
  "apps": [
    "com.google.chrome"
  ],
  "profilers": {
    "android": {
      "subject_aggregation": "default",
      "experiment_aggregation": "default",
      "sample_interval": 100,
      "data_points": [
        "cpu",
        "mem"
      ]
    },
      "batterystats": {
        "subject_aggregation": "default",
        "experiment_aggregation": "default",
        "enable_systrace_parsing": false,
        "python2_path": "python2"
        
      }
  },
  "scripts": {
    "before_experiment": "Scripts/before_experiment.py",
    "before_run": "Scripts/before_run.py",
    "after_launch": "Scripts/after_launch.py",
    "interaction": [
      {
        "type": "python3",
        "path": "Scripts/interaction.py",
        "timeout": 500,
        "logcat_regex": "some keyword"
      }
    ],
    "before_close": "Scripts/before_close.py",
    "after_run": "Scripts/after_run.py",
    "after_experiment": "Scripts/after_experiment.py"
  },
  "time_between_run": 5000
}
