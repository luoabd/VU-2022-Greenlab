{
  "type": "web",
  "devices": {
    "g9play": {}
  },
  "repetitions": 3,
  "duration": 360000,
  "systrace_path": "/var/ar-utils/systrace/systrace.py",
  "powerprofile_path": "/var/ar-utils/power_profiles/nokia_6_2.xml",
  "browsers": [
    "firefox"
  ],
  "paths": [
    "https://amazon.com/"
  ],
  "profilers": {
    "android": {
      "subject_aggregation": "default",
      "experiment_aggregation": "default",
      "sample_interval": 100,
      "data_points": [
        "cpu",
        "mem"
      ]
    },
    "batterystats": {
      "subject_aggregation": "default",
      "experiment_aggregation": "default",
      "enable_systrace_parsing": false,
      "python2_path": "/usr/bin/python2"
      
    },
    "frametimes2": {
      "subject_aggregation" : "default",
      "sample_interval": 1000
    },
    "network": { }
  },
  "scripts": {
    "before_experiment": "Scripts/before_experiment.py",
    "before_run": "Scripts/before_run.py",
    "after_launch": "Scripts/after_launch.py",
    "interaction": [
      {
        "type": "python3",
        "path": "Scripts/interaction.py"
      }
    ],
    "before_close": "Scripts/before_close.py",
    "after_run": "Scripts/after_run.py",
    "after_experiment": "Scripts/after_experiment.py"
  },
  "time_between_run": 5000
}
